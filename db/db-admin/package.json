    "@live-change/dao-vue3": "0.9.75",
    "@live-change/dao-websocket": "0.9.75",
    "@live-change/framework": "0.9.75",
    "@live-change/vue3-components": "0.9.75",
    "@live-change/vue3-ssr": "0.9.75",
    "@primeuix/themes": "^1.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vueuse/core": "^12.3.0",
    "@vueuse/head": "^2.0.0",
    "codeceptjs-assert": "^0.0.5",
    "compression": "^1.7.5",
    "cross-env": "^7.0.3",
    "find-free-ports": "^3.1.1",
    "javascript-stringify": "^2.1.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.3",
    "prism-es6": "^1.2.0",
    "prismjs": "^1.29.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-visualizer": "5.14.0",
    "serialize-javascript": "^6.0.2",
    "serve-static": "^1.16.2",
