{
  "name": "@live-change/db-admin",
  "version": "0.8.39",
  "scripts": {
    "memDev": "node server/start.js memDev --initScript ./init.js --dbAccess",
    "localDevInit": "rm tmp.db; lcli localDev --initScript ./init.js --dbAccess",
    "localDev": "node server/start.js localDev --dbAccess",
    "dev": "node server/start.js dev",
    "ssrDev": "node server/start.js ssrDev --dbAccess",
    "serveAllMem": "cross-env NODE_ENV=production node server/start.js --withApi --withServices --updateServices --withDb --dbBackend mem --createDb --dbAccess",
    "serveAll": "cross-env NODE_ENV=production node server/start.js --withApi --withServices --updateServices --dbAccess",
    "serve": "cross-env NODE_ENV=production node server/start.js --dbAccess",
    "build": "cd front; yarn build:client && yarn build:server",
    "build:client": "cd front; vite build --ssrManifest --outDir dist/client",
    "build:server": "cd front; vite build --ssr src/entry-server.js --outDir dist/server",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",
    "debug": "node --inspect-brk server"
  },
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@live-change/dao": "0.8.39",
    "@live-change/dao-message": "0.8.39",
    "@live-change/dao-sockjs": "0.8.39",
    "@live-change/dao-vue3": "0.8.39",
    "@live-change/dao-websocket": "0.8.39",
    "@live-change/framework": "0.8.39",
    "@live-change/vue3-components": "0.8.39",
    "@live-change/vue3-ssr": "0.8.39",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vueuse/core": "^10.11.0",
    "@vueuse/head": "^2.0.0",
    "codeceptjs-assert": "^0.0.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "find-free-ports": "^3.1.1",
    "javascript-stringify": "^2.1.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^3.52.0",
    "prism-es6": "^1.2.0",
    "prismjs": "^1.29.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-visualizer": "5.12.0",
    "serialize-javascript": "^6.0.2",
    "serve-static": "^1.15.0",
    "v-shared-element": "3.1.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-images": "^0.6.1",
    "vue": "^3.4.29",
    "vue-prism-editor": "2.0.0-alpha.2",
    "vue-router": "^4.3.3",
    "vue3-scroll-border": "0.1.6"
  },
