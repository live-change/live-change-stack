{
  "name": "@live-change/frontend-template",
  "version": "0.1.5",
  "scripts": {
    "memDev": "lcli memDev --enableSessions --initScript ./init.js --dbAccess",
    "localDevInit": "rm tmp.db; lcli localDev --enableSessions --initScript ./init.js",
    "localDev": "lcli localDev --enableSessions",
    "dev": "lcli dev --enableSessions",
    "ssrDev": "lcli ssrDev --enableSessions",
    "serveAllMem": "cross-env NODE_ENV=production lcli ssrServer --withApi --withServices --updateServices --enableSessions --withDb --dbBackend mem --createDb",
    "serveAll": "cross-env NODE_ENV=production lcli ssrServer --withApi --withServices --updateServices --enableSessions",
    "serve": "cross-env NODE_ENV=production lcli ssrServer --enableSessions",
    "memDev:spa": "lcli memDev --enableSessions --initScript ./init.js --dbAccess --spa",
    "localDevInit:spa": "rm tmp.db; lcli localDev --enableSessions --initScript ./init.js --spa",
    "localDev:spa": "lcli localDev --enableSessions --spa",
    "dev:spa": "lcli dev --enableSessions --spa",
    "ssrDev:spa": "lcli ssrDev --enableSessions --spa",
    "serveAllMem:spa": "cross-env NODE_ENV=production lcli server --withApi --withServices --updateServices --enableSessions --withDb --dbBackend mem --createDb --spa",
    "serveAll:spa": "cross-env NODE_ENV=production lcli server --withApi --withServices --updateServices --enableSessions --spa",
    "serve:spa": "cross-env NODE_ENV=production lcli server --enableSessions --spa",
    "apiServer": "lcli apiServer --enableSessions",
    "devApiServer": "lcli devApiServer --enableSessions",
    "memApiServer": "lcli memApiServer --enableSessions",
    "build": "cd front; yarn build:client && yarn build:server",
    "build:client": "cd front; vite build --ssrManifest --outDir dist/client",
    "build:server": "cd front; vite build --ssr src/entry-server.js --outDir dist/server",
    "build:spa": "cd front; vite build --outDir dist/spa",
    "generate": "vite build --ssrManifest --outDir dist/static && yarn build:server && node prerender",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@live-change/cli": "0.7.34",
    "@live-change/framework": "0.7.34",
    "@live-change/dao": "0.5.22",
    "@live-change/dao-vue3": "0.5.22",
    "@live-change/dao-websocket": "0.5.22",
    "@live-change/vue3-components": "0.2.31",
    "@live-change/vue3-ssr": "0.2.31",
    "@live-change/email-service": "0.3.36",
    "@live-change/access-control-service": "0.3.36",
    "@live-change/password-authentication-service": "0.3.36",
    "@live-change/secret-code-service": "0.3.36",
    "@live-change/secret-link-service": "0.3.36",
    "@live-change/session-service": "0.3.36",
    "@live-change/user-service": "0.3.36",
    "@live-change/user-identification-service": "0.3.36",
    "@live-change/prosemirror-service": "0.3.36",
    "@live-change/url-service": "0.3.36",
    "@live-change/content-service": "0.3.36",
    "@live-change/blog-service": "0.3.36",
    "@live-change/frontend-base": "0.3.16",
    "@live-change/frontend-auto-form": "0.3.16",
    "@live-change/user-frontend": "0.3.16",
    "@live-change/image-frontend": "0.3.16",
    "@live-change/upload-frontend": "0.3.16",
    "@live-change/access-control-frontend": "0.3.16",
    "@live-change/wysiwyg-frontend": "0.3.16",
    "@live-change/content-frontend": "0.3.16",
    "@live-change/blog-frontend": "0.3.16",
    "@live-change/url-frontend": "0.3.16",
    "@live-change/backup-server": "0.3.36",
    "@vueuse/core": "^10.4.1",
    "codeceptjs-assert": "^0.0.5",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "get-port-sync": "1.0.1",
    "pica": "^9.0.1",
    "pretty-bytes": "^6.0.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.40.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-visualizer": "5.9.2",
    "serialize-javascript": "^6.0.1",
    "serve-static": "^1.15.0",
    "v-shared-element": "3.1.1",
    "vue-router": "^4.2.4",
    "vue3-scroll-border": "0.1.5"
  },
  "devDependencies": {
    "@live-change/codeceptjs-helper": "0.7.34",
    "@wdio/selenium-standalone-service": "^8.15.0",
    "codeceptjs": "^3.5.4",
    "generate-password": "1.7.0",
    "playwright": "^1.37.1",
    "random-profile-generator": "^2.3.0",
    "txtgen": "^3.0.6",
    "webdriverio": "^8.16.6"
  },
  "author": "",
  "license": "ISC",
  "description": ""
}
